<fieldset>
  <div class="form-group row" >
    <div class="col-sm-1">
      <%= f.label :latitude %>
    </div>

    <div class="col-sm-3">
      <%= f.text_field :latitude, class: "form-control latitude_tee#{child_index}" %>
    </div>

    <div class="col-sm-1">
      <%= f.label :longitude %>
    </div>

    <div class="col-sm-3">
      <%= f.text_field :longitude, class: "form-control longitude_tee#{child_index}" %>
    </div>
    <%= f.hidden_field :_destroy %>
    <div class="btn btn-info create_marker" data-lat="<%= f.object.latitude %>" data-lon="<%= f.object.longitude %>" data-markerName='Diem ti' data-id="tee<%= child_index %>">Find on map</div>

    <%= link_to 'Remove', '#', class: 'remove_fields btn btn-dangger', data: { id: "tee#{child_index}" }  %>
  </div>
</fieldset>
